{
    "compilerOptions": {
        "lib": [
            "ES5",
            "ES6",
            "DOM"
        ],
        "target": "ES5",
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames":true,
        "importHelpers":true,
        "strictPropertyInitialization": false,
        "strict":true
    }
}